  "prepublishOnly": "npm run build",
  "license": "MIT",
  "dependencies": {
    "@ai-sdk/provider": "^1.1.3",
    "@ai-sdk/provider-utils": "^2.2.7",
    "zod": "^3.24.3"
  },
  "devDependencies": {
    "@types/node": "^20.11.5",
    "tsup": "^8.0.1",
    "tsx": "^4.7.0",
